{
  "name": "react-ts-liquid-gauge",
  "private": false,
  "version": "0.0.7",
  "description": "A liquid gauge component for React with type support",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./styles": "./dist/style.css"
  },
  "files": [
    "/dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "bundle": "tsup src/index.ts"
  },
  "dependencies": {
    "@types/node": "^22.13.5",
    "d3-color": "^3.1.0",
    "d3-ease": "^3.0.1",
    "d3-interpolate": "^3.0.1",
    "d3-scale": "^4.0.2",
    "d3-selection": "^3.0.0",
    "d3-shape": "^3.2.0",
    "d3-timer": "^3.0.1",
    "d3-transition": "^3.0.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tsup": "^8.4.0"
  },
  "devDependencies": {
    "@esbuild-plugins/tsconfig-paths": "^0.1.2",
    "@eslint/js": "^9.21.0",
    "@types/d3-color": "^3.1.3",
    "@types/d3-ease": "^3.0.2",
    "@types/d3-interpolate": "^3.0.4",
    "@types/d3-scale": "^4.0.9",
    "@types/d3-selection": "^3.0.11",
    "@types/d3-shape": "^3.1.7",
    "@types/d3-timer": "^3.0.2",
    "@types/d3-transition": "^3.0.9",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0"
  },
  "author": "longddh",
  "license": "MIT",
  "keywords": [
    "react",
    "typescript",
    "liquid gauge"
  ],
  "homepage": "https://github.com/longddh/react-ts-liquid-gauge",
  "repository": {
    "type": "git",
    "url": "https://github.com/longddh/react-ts-liquid-gauge"
  },
  "github": "https://github.com/longddh/react-ts-liquid-gauge"
}
